<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomLineBreakTextView">
        <attr name="gravity" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="includeFontPadding" format="boolean" />
        <attr name="textSize" format="dimension" />
        <attr name="wordHorizontalMargin" format="dimension" />
        <attr name="wordVerticalMargin" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="letterSpacing" format="float" />
    </declare-styleable>
</resources>